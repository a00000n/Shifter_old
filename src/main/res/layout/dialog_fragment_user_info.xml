<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp">

    <include layout="@layout/text_dialog_title" />

    <TextView
        android:id="@+id/NULL_DF_UI_name_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:text="@string/mgr_accmgr_df_name_title"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/DF_UI_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/NULL_DF_UI_email_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:text="@string/mgr_accmgr_df_email_title"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/DF_UI_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/NULL_DF_UI_phone_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:text="@string/mgr_accmgr_df_phone_title"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/DF_UI_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/NULL_DF_UI_job_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:text="@string/mgr_accmgr_df_job_title"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Spinner
        android:id="@+id/DF_UI_job_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:textAlignment="gravity" />

    <TextView
        android:id="@+id/NULL_DF_UI_rating"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:text="@string/mgr_accmgr_df_rating"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Spinner
        android:id="@+id/DF_UI_rating"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|start"
        android:textAlignment="gravity" />

    <Button
        android:id="@+id/DF_UI_delete_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mgr_accmgr_df_delete" />

    <Button
        android:id="@+id/DF_UI_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/back" />


</LinearLayout>